[32m+[m[32mdef start():[m
[32m+[m
[32m+[m[32m    import nuke[m
[32m+[m[32m    import nukescripts[m
[32m+[m
[32m+[m[32m    class SetFirstFramePanel(nukescripts.PythonPanel):[m
[32m+[m[32m        def __init__(self):[m
[32m+[m[32m            nukescripts.PythonPanel.__init__(self, 'Merge Transforms')[m
[32m+[m
[32m+[m[32m            # CREATE KNOBS[m
[32m+[m[32m            self.first = nuke.Int_Knob('first', 'First Frame')[m
[32m+[m[32m            self.first.setValue(int(nuke.root()['first_frame'].value()))[m
[32m+[m
[32m+[m[32m            # ADD KNOBS[m
[32m+[m[32m            self.addKnob(self.first)[m
[32m+[m
[32m+[m[32m    # open panel and get value for first frame[m
[32m+[m[32m    p = SetFirstFramePanel()[m
[32m+[m[32m    if p.showModalDialog():[m
[32m+[m[32m        first = p.first.value()[m
[32m+[m
[32m+[m[32m    # set first frame into nodes[m
[32m+[m[32m    nodes = nuke.selectedNodes()[m
[32m+[m[32m    for i in nodes:[m
[32m+[m[32m        if i.Class() == "Read":[m
[32m+[m[32m            i.knob('frame_mode').setValue("start at")[m
[32m+[m[32m            i.knob('frame').setValue(str(first))[m
[32m+[m[32m        else:[m
[32m+[m[32m            nuke.message("It has to be Read node")[m
[32m+[m[32m            return 0[m
