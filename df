[32m+[m[32m#----------------------------------------------------------------------------------------------------------[m
[32m+[m[32m# Wouter Gilsing[m
[32m+[m[32m# woutergilsing@hotmail.com[m
[32m+[m[32m# January 2016[m
[32m+[m[32m# v1.0[m
[32m+[m[32m#----------------------------------------------------------------------------------------------------------[m
[32m+[m[32mimport nuke[m
[32m+[m
[32m+[m[32mdef reformThat():[m
[32m+[m[32m    '''[m
[32m+[m[32m    Quickly set the format knob on a reformatnode to the format of an other node. If no reformat node is selected,[m
[32m+[m[32m    a new one will be created.[m
[32m+[m[32m    '''[m
[32m+[m[32m    selection = nuke.selectedNodes()[m
[32m+[m[41m    [m
[32m+[m[32m    #if you've nothing selected you probably missclicked while trying to create a regular reformat[m
[32m+[m[32m    #so I'll be nice and create a regular reformat for you anyway and do nothing else[m
[32m+[m[32m    if len(selection) < 1:[m
[32m+[m[32m        nuke.createNode('Reformat')[m
[32m+[m[32m        return[m
[32m+[m[41m    [m
[32m+[m[32m    #if you selected a group of nodes, I'm not sure which node to steal the format from. So please try again[m
[32m+[m[32m    if len(selection) > 2:[m
[32m+[m[32m        nuke.message('please select only two nodes')[m
[32m+[m[32m        return[m
[32m+[m
[32m+[m[32m    #if you've only one node selected, we have something to steal from but nothing to apply it to..[m
[32m+[m[32m    #so create a new reformatnode[m
[32m+[m[41m    [m
[32m+[m[32m    reformatNode = ''[m
[32m+[m[41m    [m
[32m+[m[32m    if len(selection) == 1:[m
[32m+[m[32m        selection[0].knob('selected').setValue(False)[m
[32m+[m[32m        reformatNode = nuke.createNode('Reformat')[m
[32m+[m[41m        [m
[32m+[m[32m    #define the reformat node[m
[32m+[m[32m    for i in selection:[m
[32m+[m[32m        if i.Class() == 'Reformat' and reformatNode == '':[m
[32m+[m[32m            reformatNode = i[m
[32m+[m[32m            selection.remove(i)[m
[32m+[m[32m            break[m
[32m+[m[41m    [m
[32m+[m[32m    #if none of the selected nodes is a reformat. Check the nodes for a format knob.[m
[32m+[m[32m    #if one of the nodes happens to have one, use that node.[m[41m [m
[32m+[m[32m    if len(selection) == 2:[m
[32m+[m[32m        for i in selection[::-1]:[m
[32m+[m	[32m    if i.Class() != 'Read' and 'format' in i.knobs():[m
[32m+[m	[32m        reformatNode = i[m
[32m+[m[32m                selection.remove(i)[m
[32m+[m[32m                break[m
[32m+[m[41m                [m
[32m+[m[32m    #if you've multiple nodes selected at least one should be formatable.[m[41m [m
[32m+[m[32m    #Otherwise we have plenty of selected sources to steal our formats from, but nowhere to store are just acquired treasures[m
[32m+[m[32m    if len(selection) == 2:[m
[32m+[m[32m        nuke.message('If you select multiple nodes, make sure at least one is a reformat or has an formatknob')[m
[32m+[m[32m        return[m
[32m+[m[41m        [m
[32m+[m[32m    #define the node you want to match the format from[m
[32m+[m[32m    targetNode = selection[0][m
[32m+[m[41m     [m
[32m+[m[32m    #built a dictionary of all the existing formats in the scene[m
[32m+[m[32m    formatsDict = {}[m
[32m+[m[32m    for i in nuke.formats():[m
[32m+[m[32m        if i.name() != None:[m
[32m+[m[32m            formatsDict[(i.width(), i.height(),i.pixelAspect())]=i.name()[m
[32m+[m
[32m+[m[32m    targetFormat = (targetNode.format().width(), targetNode.format().height(), targetNode.format().pixelAspect())[m
[32m+[m
[32m+[m[32m    #check whether the format you're trying to copy already exists in your script[m
[32m+[m[32m    if targetFormat in formatsDict:[m
[32m+[m[32m        name = formatsDict[targetFormat][m
[32m+[m
[32m+[m[32m    #if that's not the case, add it.[m
[32m+[m[32m    else:[m
[32m+[m[32m        name = 'custom 01'[m
[32m+[m[32m        counter = 2[m
[32m+[m[32m        while name in formatsDict.values():[m
[32m+[m[32m            name = 'custom ' + str(counter).zfill(2)[m
[32m+[m[32m            counter += 1[m
[32m+[m
[32m+[m[32m        nuke.addFormat(' '.join(map(str,targetFormat)) + ' ' + name)[m
[32m+[m
[32m+[m[32m    #set the reformatnode to new format[m
[32m+[m[32m    reformatNode.knob('format').setValue(name)[m
[32m+[m
[32m+[m[32mreformThat()[m
