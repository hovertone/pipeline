[32m+[m[32m    def printError(self, error):[m
[32m+[m
[32m+[m[32m        fullError = error.splitlines()[m
[32m+[m
[32m+[m[32m        lineNumber = 'error determining line'[m
[32m+[m
[32m+[m[32m        for index, line in enumerate(reversed(fullError)):[m
[32m+[m[32m            if line.startswith('  File "<'):[m
[32m+[m
[32m+[m[32m                for i in line.split(','):[m
[32m+[m[32m                    if i.startswith(' line '):[m
[32m+[m[32m                        lineNumber = i[m
[32m+[m
[32m+[m[32m                index = len(fullError)-index[m
[32m+[m[32m                break[m
[32m+[m
[32m+[m[32m        fullError = fullError[index:][m
[32m+[m
[32m+[m[32m        errorDescription = '\n'.join(fullError)[m
[32m+[m
[32m+[m[32m        scriptFolder = os.path.dirname(self.filePath)[m
[32m+[m[32m        scriptFolderName = os.path.basename(scriptFolder)[m
[32m+[m
[32m+[m[32m        buttonName = [self.text()][m
[32m+[m
[32m+[m[32m        while len(scriptFolderName) == 3 and scriptFolderName.isdigit():[m
[32m+[m
[32m+[m[32m            name = open(scriptFolder+'/_name.json').read()[m
[32m+[m[32m            buttonName.insert(0, name)[m
[32m+[m[32m            scriptFolder = os.path.dirname(scriptFolder)[m
[32m+[m[32m            scriptFolderName = os.path.basename(scriptFolder)[m
[32m+[m
[32m+[m[32m        for i in range(2):[m
[32m+[m[32m            buttonName.insert(0, os.path.basename(scriptFolder))[m
[32m+[m[32m            scriptFolder = os.path.dirname(scriptFolder)[m
[32m+[m
[32m+[m[32m        hotboxError = '\nW_HOTBOX ERROR: %s -%s:\n\n%s'%('/'.join(buttonName),lineNumber,errorDescription)[m
[32m+[m
[32m+[m[32m        #print error[m
[32m+[m[32m        print(hotboxError)[m
[32m+[m[32m        nuke.tprint(hotboxError)[m
